[
  {
    "title": "画", 
    "notes": [], 
    "author": "王维", 
    "paragraphs": [
      "远看山有色，", 
      "近听水无声。", 
      "春去花还在，",
      "人来鸟不惊。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "静夜思", 
    "notes": [], 
    "author": "李白", 
    "paragraphs": [
      "床前明月光，", 
      "疑是地上霜。", 
      "举头望明月，",
      "低头思故乡。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "咏鹅", 
    "notes": [], 
    "author": "骆宾王", 
    "paragraphs": [
      "鹅，鹅，鹅，", 
      "曲项向天歌。", 
      "白毛浮绿水，",
      "红掌拨清波。"
    ], 
    "dynasty": "唐代"
  }
]