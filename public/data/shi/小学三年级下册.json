[
  {
    "title": "咏柳 / 柳枝词", 
    "notes": [], 
    "author": "贺知章", 
    "paragraphs": [
      "碧玉妆成一树高，",
      "万条垂下绿丝绦。", 
      "不知细叶谁裁出，",
      "二月春风似剪刀。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "春日", 
    "notes": [], 
    "author": "朱熹", 
    "paragraphs": [
      "胜日寻芳泗水滨，",
      "无边光景一时新。", 
      "等闲识得东风面，",
      "万紫千红总是春。"
    ], 
    "dynasty": "宋代"
  }, 
  {
    "title": "嫦娥", 
    "notes": [], 
    "author": "李商隐", 
    "paragraphs": [
      "云母屏风烛影深，",
      "长河渐落晓星沉。", 
      "嫦娥应悔偷灵药，",
      "碧海青天夜夜心。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "乞巧", 
    "notes": [], 
    "author": "林杰", 
    "paragraphs": [
      "七夕今宵看碧霄，",
      "牵牛织女渡河桥。", 
      "家家乞巧望秋月，",
      "穿尽红丝几万条。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "游子吟", 
    "notes": [], 
    "author": "孟郊", 
    "paragraphs": [
      "慈母手中线，",
      "游子身上衣。", 
      "临行密密缝，",
      "意恐迟迟归。", 
      "谁言寸草心，",
      "报得三春晖。"
    ], 
    "dynasty": "唐代"
  }
]