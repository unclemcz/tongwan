[
  {
    "title": "天竺寺八月十五日夜桂子", 
    "notes": [], 
    "author": "皮日休", 
    "paragraphs": [
      "玉颗珊珊下月轮，","殿前拾得露华新。", 
      "至今不会天中事，","应是嫦娥掷与人。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "七步诗", 
    "notes": [
      "菽--一作 ：豉"
    ], 
    "author": "曹植", 
    "paragraphs": [
      "版本二：", 
      "煮豆燃豆萁，","豆在釜中泣。", 
      "本是同根生，","相煎何太急？", 
      "版本二：", 
      "煮豆持作羹，","漉菽以为汁。", 
      "萁在釜下燃，","豆在釜中泣。", 
      "本自同根生，","相煎何太急？"
    ], 
    "dynasty": "两汉"
  }, 
  {
    "title": "闻官军收河南河北", 
    "notes": [], 
    "author": "杜甫", 
    "paragraphs": [
      "剑外忽传收蓟北，","初闻涕泪满衣裳。", 
      "却看妻子愁何在，","漫卷诗书喜欲狂。", 
      "白日放歌须纵酒，","青春作伴好还乡。", 
      "即从巴峡穿巫峡，","便下襄阳向洛阳。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "江畔独步寻花·其六", 
    "notes": [], 
    "author": "杜甫", 
    "paragraphs": [
      "黄四娘家花满蹊，","千朵万朵压枝低。", 
      "留连戏蝶时时舞，","自在娇莺恰恰啼。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "鸟鸣涧", 
    "notes": [], 
    "author": "王维", 
    "paragraphs": [
      "人闲桂花落，","夜静春山空。", 
      "月出惊山鸟，","时鸣春涧中。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "元日", 
    "notes": [], 
    "author": "王安石", 
    "paragraphs": [
      "爆竹声中一岁除，","春风送暖入屠苏。", 
      "千门万户曈曈日，","总把新桃换旧符。"
    ], 
    "dynasty": "宋代"
  }, 
  {
    "title": "浣溪沙·游蕲水清泉寺", 
    "notes": [
      "潇潇--一作 ：萧萧"
    ], 
    "author": "苏轼", 
    "paragraphs": [
      "游蕲水清泉寺，","寺临兰溪，溪水西流。", 
      "", 
      "山下兰芽短浸溪，","松间沙路净无泥，","潇潇暮雨子规啼。", 
      "谁道人生无再少？","门前流水尚能西！","休将白发唱黄鸡。"
    ], 
    "dynasty": "宋代"
  }, 
  {
    "title": "石灰吟", 
    "notes": [
      "千锤万凿出深山--一作 ：千锤万击出深山 / 千鎚万击出深山", 
      "粉骨碎身全不怕--一作 ：粉骨碎身全不惜 / 粉身碎骨浑不怕 / 粉身碎骨全不怕"
    ], 
    "author": "于谦", 
    "paragraphs": [
      "千锤万凿出深山，","烈火焚烧若等闲。", 
      "粉骨碎身全不怕，","要留清白在人间。", 
      "", 
      "", 
      ""
    ], 
    "dynasty": "明代"
  }, 
  {
    "title": "竹石", 
    "notes": ["(坚劲 一作：韧)"], 
    "author": "郑燮", 
    "paragraphs": [
      "咬定青山不放松，","立根原在破岩中。", 
      "千磨万击还坚劲，","任尔东西南北风。"
    ], 
    "dynasty": "清代"
  }, 
  {
    "title": "芙蓉楼送辛渐", 
    "notes": [], 
    "author": "王昌龄", 
    "paragraphs": [
      "寒雨连江夜入吴，","平明送客楚山孤。", 
      "洛阳亲友如相问，","一片冰心在玉壶。"
    ], 
    "dynasty": "唐代"
  }
]
