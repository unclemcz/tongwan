[
  {
    "title": "乡村四月", 
    "notes": [], 
    "author": "翁卷", 
    "paragraphs": [
      "绿遍山原白满川，",
      "子规声里雨如烟。", 
      "乡村四月闲人少，",
      "才了蚕桑又插田。"
    ], 
    "dynasty": "宋代"
  }, 
  {
    "title": "独坐敬亭山", 
    "notes": [
      "只有--一作 ：惟 / 唯"
    ], 
    "author": "李白", 
    "paragraphs": [
      "众鸟高飞尽，",
      "孤云独去闲。", 
      "相看两不厌，",
      "只有敬亭山。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "忆江南·江南好", 
    "notes": [], 
    "author": "白居易", 
    "paragraphs": [
      "江南好，风景旧曾谙。",
      "日出江花红胜火，",
      "春来江水绿如蓝。",
      "能不忆江南？"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "望洞庭", 
    "notes": [
      "山水色--一作 ：山水翠"
    ], 
    "author": "刘禹锡", 
    "paragraphs": [
      "湖光秋月两相和，",
      "潭面无风镜未磨。", 
      "遥望洞庭山水色，",
      "白银盘里一青螺。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "渔歌子·西塞山前白鹭飞", 
    "notes": [], 
    "author": "张志和", 
    "paragraphs": [
      "西塞山前白鹭飞，",
      "桃花流水鳜鱼肥。", 
      "青箬笠，绿蓑衣，",
      "斜风细雨不须归。"
    ], 
    "dynasty": "唐代"
  }, 
  {
    "title": "四时田园杂兴·其二", 
    "notes": [
      "惟--通 唯"
    ], 
    "author": "范成大", 
    "paragraphs": [
      "梅子金黄杏子肥，",
      "麦花雪白菜花稀。", 
      "日长篱落无人过，",
      "惟有蜻蜓蛱蝶飞。"
    ], 
    "dynasty": "宋代"
  }
]